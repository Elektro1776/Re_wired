{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        },
        "debug": false
      }
    ],
    "babel-preset-react"
  ],
  "plugins": [
    [
      "react-hot-loader/babel",
      {
        "safetyNet": true
      }
    ],
    "transform-class-properties",
    "transform-runtime",
  ]
}
